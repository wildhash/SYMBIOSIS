{
  "name": "@symbiosis/agents",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@symbiosis/shared": "workspace:*",
    "@symbiosis/kernel": "workspace:*",
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/babel__traverse": "^7.20.0",
    "@babel/types": "^7.24.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  }
}
